{
	"name": "html-element-map",
	"version": "1.4.0",
	"description": "Look up HTML tag names via HTML Element constructors, and vice versa.",
	"main": "index.js",
	"exports": {
		".": [
			{
				"import": "./index.mjs",
				"require": "./index.js",
				"default": "./index.js"
			},
			"./index.js"
		],
		"./": "./index.js",
		"./index": "./index.js",
		"./index.js": "./index.js",
		"./byTag": "./byTag.js",
		"./byTag.js": "./byTag.js",
		"./byConstructor": "./byConstructor.js",
		"./byConstructor.js": "./byConstructor.js",
		"./byConstructorName": "./byConstructorName.js",
		"./byConstructorName.js": "./byConstructorName.js",
		"./getData": "./getData.js",
		"./getData.js": "./getData.js",
		"./package": "./package.json",
		"./package.json": "./package.json",
		"./test": "./test/index.js",
		"./test/": "./test/"
	},
	"scripts": {
		"prepack": "npmignore --auto --commentLines=auto",
		"prepublishOnly": "safe-publish-latest",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"pretest": "npm run lint",
		"tests-only": "nyc tape 'test/**/*.js'",
		"tests-esm": "nyc node test/index.mjs",
		"test": "npm run tests-only && npm run tests-esm",
		"posttest": "npx npm@\">= 10.2\" audit --production",
		"lint": "eslint .",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ljharb/html-element-map.git"
	},
	"keywords": [
		"html",
		"element",
		"tag",
		"lookup",
		"html",
		"element",
		"constructor"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/html-element-map/issues"
	},
	"homepage": "https://github.com/ljharb/html-element-map#readme",
	"devDependencies": {
		"@ljharb/eslint-config": "^22.1.3",
		"auto-changelog": "^2.5.0",
		"encoding": "^0.1.13",
		"eslint": "^9.39.2",
		"for-each": "^0.3.5",
		"function.prototype.name": "^1.1.8",
		"isarray": "^2.0.5",
		"jiti": "^0.0.0",
		"npmignore": "^0.3.5",
		"nyc": "^10.3.2",
		"object-inspect": "^1.13.4",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.9.0"
	},
	"dependencies": {
		"array.prototype.filter": "^1.0.4",
		"es-errors": "^1.3.0"
	},
	"testling": {
		"files": "test/index.js"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false
	},
	"publishConfig": {
		"ignore": [
			".github/workflows"
		]
	},
	"engines": {
		"node": ">= 0.4"
	}
}
